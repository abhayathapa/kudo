<p id="notice"><%= notice %></p>

<h1>Stand Ups</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stand_ups.each do |stand_up| %>
      <tr>
        <td><%= link_to 'Show', stand_up %></td>
        <td><%= link_to 'Edit', edit_stand_up_path(stand_up) %></td>
        <td><%= link_to 'Destroy', stand_up, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stand Up', new_stand_up_path %>
